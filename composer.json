{
    "name": "terabytesoft/app-template-basic",
    "type": "project",
    "description": "Yii 3.0 Project Skeleton Application",
    "keywords": [
        "application", 
        "project",
        "skeleton",
        "yii3"
    ],
    "homepage": "https://github.com/terabytesoft/app-template-basic",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/terabytesoft/app-template-basic/issues",
        "source": "https://github.com//terabytesoft/app-template-basic",
        "forum":  "https://discuss.cjtterabyte.com"
    },
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "terabytesoftw@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/cache": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "terabytesoft/app-basic": "^0.0.1@dev"
    },
    "require-dev": {
        "yiisoft/yii-debug": "^3.0@dev",
        "foxy/foxy": "^1.0",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "config": {
        "foxy": {
            "enable-packages": {
                "*": true
            }
        },
        "fxp-asset": {
            "enabled": false
        },
        "process-timeout": 1800
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        },
        "config-plugin": {
            "common": "config/common.php",
            "params": "config/params.php"
        }
    }
}
