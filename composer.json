{
    "name": "terabytesoft/app-template-basic",
    "type": "project",
    "description": "Yii 3.0 Project Skeleton Application",
    "keywords": [
        "application",
        "project",
        "skeleton",
        "yii3"
    ],
    "homepage": "https://github.com/terabytesoft/app-template-basic",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "wilmer.arambula@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
		"php": "^7.2",
		"foxy/foxy": "^1.0",
        "hiqdev/composer-config-plugin": "^1.0@dev",
        "nyholm/psr7": "^1.0",
        "terabytesoft/app-basic": "^0.0.1@dev",
        "yiisoft/cache": "dev-master#b10f733",
        "yiisoft/di": "^3.0@dev",
		"yiisoft/log": "^3.0@dev",
        "yiisoft/log-target-file": "^3.0@dev",
        "yiisoft/rbac": "dev-master#2f0194e",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/view": "^3.0@dev",
        "yiisoft/yii-core": "dev-master#ac80ebf",
        "yiisoft/yii-web": "dev-master#4d5d840"
    },
    "require-dev": {
		"codeception/codeception": "^3.0",
        "yiisoft/yii-debug": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "TerabyteSoft\\App\\Template\\Basic\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": "config/common.php",
            "tests": "config/tests.php"
        }
    }
}
